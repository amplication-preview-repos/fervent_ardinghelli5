datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Colo {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  title     String?
  updatedAt DateTime @updatedAt
}

model Post {
  createdAt  DateTime @default(now())
  descritpin String?
  id         String   @id @default(cuid())
  title      String?
  updatedAt  DateTime @updatedAt
}
